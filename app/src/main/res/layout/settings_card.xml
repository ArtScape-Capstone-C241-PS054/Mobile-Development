<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <View
        android:id="@+id/bg_transaction_title"
        android:layout_width="match_parent"
        android:layout_height="28dp"

        card_view:layout_constraintTop_toTopOf="parent"
        card_view:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/tv_transaction_title"
        style="@style/light"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/transaction"
        android:layout_marginStart="16dp"

        card_view:layout_constraintBottom_toBottomOf="@+id/bg_transaction_title"
        card_view:layout_constraintTop_toTopOf="@+id/bg_transaction_title" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cv_notification"
        card_view:strokeWidth="0dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        card_view:cardElevation="0dp"
        card_view:layout_constraintTop_toBottomOf="@+id/bg_transaction_title">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/CardViewStyle">

            <ImageView
                android:id="@+id/notif_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="360dp"
                android:src="@drawable/notification"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_notification"
                style="@style/semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"

                android:text="@string/notification"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@drawable/ic_arrow_forward"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />



        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cv_transaction_history"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/transparant"
        card_view:cardElevation="0dp"
        card_view:strokeWidth="0dp"

        card_view:layout_constraintTop_toBottomOf="@+id/cv_notification">

        <androidx.constraintlayout.widget.ConstraintLayout

            style="@style/CardViewStyle"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/transaction_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="360dp"
                android:src="@drawable/receipt"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_transaction_history"
                style="@style/semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"

                android:text="@string/transaction_history"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="1.0" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@drawable/ic_arrow_forward"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <View
        android:id="@+id/bg_general_title"
        android:layout_width="match_parent"

        android:layout_height="28dp"


        card_view:layout_constraintTop_toBottomOf="@+id/cv_transaction_history"
        card_view:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/tv_general_title"
        style="@style/light"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/general"

        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"

        card_view:layout_constraintBottom_toBottomOf="@+id/bg_general_title"
        card_view:layout_constraintTop_toTopOf="@+id/bg_general_title" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cv_mypainting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/transparant"
        card_view:strokeWidth="0dp"
        card_view:cardElevation="0dp"
        card_view:layout_constraintTop_toBottomOf="@+id/tv_general_title">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/CardViewStyle">

            <ImageView
                android:id="@+id/painting_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="360dp"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_mypainting"
                style="@style/semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"
                android:text="My Painting"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@drawable/ic_arrow_forward"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cv_address"
        android:layout_width="match_parent"
         android:layout_height="wrap_content"
        android:backgroundTint="@color/transparant"
        card_view:strokeWidth="0dp"
        card_view:cardElevation="0dp"
        card_view:layout_constraintTop_toBottomOf="@+id/cv_mypainting">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/CardViewStyle">

            <ImageView
                android:id="@+id/address_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="360dp"
                android:src="@drawable/place"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_address"
                style="@style/semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"
                android:text="@string/address"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@drawable/ic_arrow_forward"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>





    <View
        android:id="@+id/bg_about_title"
        android:layout_width="match_parent"
        android:layout_height="28dp"
        card_view:layout_constraintTop_toBottomOf="@+id/cv_address"
        card_view:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/tv_about_title"
        style="@style/light"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/about"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        card_view:layout_constraintBottom_toBottomOf="@+id/bg_about_title"
        card_view:layout_constraintTop_toTopOf="@+id/bg_about_title" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cv_support"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/transparant"
        card_view:strokeWidth="0dp"
        card_view:cardElevation="0dp"
        card_view:layout_constraintTop_toBottomOf="@+id/bg_about_title">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/CardViewStyle">

            <ImageView
                android:id="@+id/support_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="360dp"
                android:src="@drawable/support"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_support"
                style="@style/semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"

                android:text="@string/support"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@drawable/ic_arrow_forward"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cv_about_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/transparant"
        card_view:strokeWidth="0dp"
        card_view:cardElevation="0dp"
        card_view:layout_constraintTop_toBottomOf="@+id/cv_support">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/CardViewStyle">

            <ImageView
                android:id="@+id/about_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="360dp"
                android:src="@drawable/info"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_about_app"
                style="@style/semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"

                android:text="@string/about_app"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@drawable/ic_arrow_forward"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <View
        android:id="@+id/bg_logout_title"
        android:layout_width="match_parent"

        android:layout_height="28dp"


        card_view:layout_constraintTop_toBottomOf="@+id/cv_about_app"
        card_view:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/tv_logout_title"
        style="@style/light"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/logout"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        card_view:layout_constraintBottom_toBottomOf="@+id/bg_logout_title"
        card_view:layout_constraintTop_toTopOf="@+id/bg_logout_title" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cv_logout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/transparant"
        card_view:strokeWidth="0dp"

        card_view:cardElevation="0dp"
        card_view:layout_constraintTop_toBottomOf="@+id/bg_logout_title">

        <androidx.constraintlayout.widget.ConstraintLayout

            style="@style/CardViewStyle"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/logout_icon"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="360dp"
                android:src="@drawable/logout"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_logout"
                style="@style/semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"

                android:text="@string/logout"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="1.0" />

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:src="@drawable/ic_arrow_forward"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
</androidx.constraintlayout.widget.ConstraintLayout>